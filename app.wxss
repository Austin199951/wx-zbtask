/* 公用代码 */
page{font-size:30rpx;}
input{font-size: 28rpx;}
.font_size32{font-size:32rpx;}
.font_size24{font-size:24rpx;}
.font_size34{font-size:34rpx;}
.font_size36{font-size:36rpx;}
.font_size30{font-size:30rpx;}
.font_size28{font-size:28rpx;}
.wrap_text{word-break:break-all;}
.clear::after{display: block;content: "";clear: both;}
.flex_start{display: flex;justify-content: flex-start;}
.flex_start_center{display: flex;justify-content: flex-start;align-items:center;flex-flow: wrap;}
.flex_center{display: flex;justify-content: center;align-items: center;}
.flex_space_bet{display:flex;justify-content: space-between;align-items: center;}
.flex_space_bet_wrap{display:flex;justify-content: space-between;align-items: center;flex-flow: wrap;}
.flex_end{display: flex;justify-content: flex-end;align-items: center;flex-flow: wrap;}
.flex_end_start{display: flex;justify-content: flex-end;align-items: flex-start;}
.flex_column{display: flex;flex-flow:column;}
.w100{width:100%;}
.h100{height:100%;}
.w95{width:95%;margin:0 auto;}
.w90{width:90%;margin:0 auto;}
.w85{width:85%;margin:0 auto;}
.w80{width:80%;margin:0 auto;}
