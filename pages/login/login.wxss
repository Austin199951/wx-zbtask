.container{
  padding:0  40rpx 45rpx;
}
.register{
  padding:45rpx 10rpx 45rpx 0;
  text-align: right;
  font-size: 34rpx; 
  line-height: 40rpx;
}
.login_title{
  font-size:60rpx;
}
.tab_btn{
  width: 100%;
  height: 80rpx;
  margin-top: 67rpx;
  line-height: 80rpx;
  background-color: #e3e3e3;
}
.quick_register{border:1px solid #efefef;border-radius:0 0 20rpx 20rpx;  }
.tab_btn_cur{
   background-color:#ffdb32;
}
.tab_btn:nth-child(1){
  border-top-left-radius: 20rpx;  
  border-top-right-radius: 20rpx;  
}
.tab_btn:nth-child(2){
  border-top-right-radius: 20rpx;
}
.list{
  padding: 40rpx 0;
}
.icon{
  width: 45rpx;
  height: 45rpx;
  margin-right: 10rpx;
}
.tips{
  font-size: 24rpx;
  line-height: 68rpx;
}
.radio{
  width:12rpx;
  height: 12rpx;
  border-radius: 50%;
  border: 1rpx solid #ff7171;
  margin-right: 10rpx;
}
.radio.check{
  background: #ff7171;
}
.login_btn{
  margin:80rpx auto;
}
.thirdlogin{
  flex-wrap:wrap 
}
.line{
  width: 200rpx;
  border-top: 1px solid #efefef;
}
.wxlogin{
  padding:35rpx 0;
}
.pw_icon{width:50rpx;height:50rpx;margin-right:18rpx;}
.login_icon{
  width: 108rpx;
  height: 108rpx;
  display: block
}
.serviceAgreement{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.3);
  z-index: 10;
}
.service_wrap{
  width: 547rpx;
  height: 654rpx;
  border-radius: 10rpx;
  box-sizing: border-box
}
.service_title{line-height: 86rpx;border-bottom: 1rpx solid #ffdb32;}
.service_content{height:470rpx;letter-spacing: 1rpx;line-height: 45rpx;box-sizing: border-box;color:#666;}
.service_content::-webkit-scrollbar{display: none;}